generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model GuestMessage {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  name       String
  message    String
  ip         String
  createdAt  DateTime @default(now())
}